
{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "nextApp"
      }
    ]
  },
  "functions": {
    "source": ".",
    "runtime": "nodejs20",
    "ignore": [
      "node_modules/.cache",
      ".firebase/**",
      ".firebaserc",
      ".git/**",
      ".github/**",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "*.log",
      "fleetflowsub/**",
      "src/components/ui/**",
      "src/contexts/**",
      "src/hooks/**",
      "src/lib/firebase.ts",
      "src/ai/**",
      "README.md",
      "apphosting.yaml",
      "components.json",
      "tailwind.config.ts",
      "tsconfig.json",
      "src/app/about/**",
      "src/app/contact/**",
      "src/app/api/**",
      "src/app/dashboard/**",
      "src/app/flow-login/**",
      "src/app/login/**",
      "src/app/not-found.tsx"
    ]
  }
}
